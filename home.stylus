body
	background #333
	font-family Helvetica, Arial
	font-size 16px

h1
	background white
	border-radius 0.5em	
	color #333
	padding 0.5em
	text-align right
	text-shadow 5px 5px 10px rgba(0,0,0, 0.5)

#about
	background white
	border-radius 0.5em	
	margin 1em auto
	padding 1em 0
	vertical-align top

#about article	
	display inline-block
	margin 0 2%
	min-height 290px
	padding 0.25em
	vertical-align top
	width 28%

#menu
	text-align right
	width 95%

